@page
@{
    Layout = null;
}

<div class="modal-dialog modal-dialog-centered">
    <div class="modal-content">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="staticBackdropLabel">Cancelar reserva</h5>
                <button type="button" class="btn-close" data-coreui-dismiss="modal" aria-label="Close"></button>
            </div>
            <div class="modal-body">
                <span class="fs-6">¿Está seguro de cancelar la reserva?</span>
            </div>
            <div class="modal-footer">
                <input type="button" class="btn btn-secondary" data-coreui-dismiss="modal" value="Cancelar" />
                <input type="submit" class="btn btn-primary btn-submit-remove" value="Aceptar" />
            </div>
        </div>
    </div>
</div>