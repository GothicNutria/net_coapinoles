@page
@section HeadContent {
    <link rel="stylesheet" href="~/lib/MCDatepicker-master/dist/mc-calendar.min.css" />
    <link rel="stylesheet" href="~/lib/data-table/datatables.min.css" />
}
<nav aria-label="breadcrumb">
    <ol class="breadcrumb ">
        <li class="breadcrumb-item" aria-current="page">Sistema</li>
        <li class="breadcrumb-item active" aria-current="page">Reservaciones</li>
    </ol>
</nav>
<div class="row g-3">
    <div class="col">
        <h1 class="display-6">
            Reservaciones
        </h1>
    </div>
    <div class="col-auto">
        <a class="btn btn-primary position-relative top-50 translate-middle-y" href="./Create/">
            <i class="me-1 bi bi-plus-lg"></i> Nueva reservación
        </a>
    </div>
    <div class="col-12">
        <partial name="/Pages/Shared/Components/Table.cshtml" model='new string[]{
            "",
            "Fecha",
            "Confirmación",
            "Cliente",
            "Celular",
            "Transporte",
            "Adulto",
            "Menor",
            "Usuario"
        }' />
    </div>
</div>
<div class="modal fade" id="modalServices" tabindex="-1">
    <div class="modal-dialog modal-content"></div>
</div>

<partial name="/Pages/Shared/Components/loadScreen.cshtml" />

@section ScriptsContent {
    <script type="module" src="~/js/reservations/page.js"></script>
}